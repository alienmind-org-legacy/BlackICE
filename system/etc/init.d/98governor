# Default governor settings
echo "ondemandx" > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor
echo "614400"    > /sys/devices/system/cpu/cpufreq/ondemandx/suspend_freq
echo "15"        > /sys/devices/system/cpu/cpufreq/ondemandx/down_differential
echo "50"        > /sys/devices/system/cpu/cpufreq/ondemandx/sampling_down_factor
echo "0"         > /sys/devices/system/cpu/cpufreq/ondemandx/ignore_nice_load
echo "85"        > /sys/devices/system/cpu/cpufreq/ondemandx/up_threshold
echo "50"        > /sys/devices/system/cpu/cpufreq/ondemandx/powersave_bias
echo "80000"     > /sys/devices/system/cpu/cpufreq/ondemandx/sampling_rate
echo "1"         > /sys/devices/system/cpu/cpufreq/ondemandx/io_is_busy
